<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="MyConn" 
         connectionString="Data Source=.;Initial Catalog=OnlineShoppingSite;Integrated Security=True"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <system.web>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
    <authentication mode="Forms">
      <forms loginUrl="~/Login.aspx" defaultUrl="~/Default.aspx" timeout="2880" />
    </authentication>
    <sessionState mode="InProc" timeout="20" />
  </system.web>
</configuration> 